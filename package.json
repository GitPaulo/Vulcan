{
    "name": "Vulcan",
    "version": "1.0.0",
    "description": "Collaboration discord bot by Paulo & Carlos, using discord.js and heroku.",
    "main": "bot.js",
    "scripts": {
        "_COMMENT0": " ============================ BUILD SCRIPTS  ============================",
        "wintools": "npm install -g windows-build-tools",
        "_COMMENT1": " ============================ RUN SCRIPTS  ============================",
        "start": "node --require ./plugins/scripts/globals.js bot.js",
        "dev": "nodemon --require ./plugins/scripts/globals.js bot.js",
        "_COMMENT2": " ============================ TEST SCRIPTS  ============================",
        "test": "node --require ./plugins/scripts/globals.js tests/loading.js",
        "test:music": "node --require ./plugins/scripts/globals.js tests/musicDependencies.js",
        "_COMMENT3": " ============================ CODE ENV SCRIPTS  ============================",
        "linter": "./node_modules/.bin/eslint .",
        "linter:errors-only": "./node_modules/.bin/eslint . --quiet",
        "linter:setup": "npm install eslint --save-dev",
        "linter:init": "./node_modules/.bin/eslint --init",
        "linter:fix": "./node_modules/.bin/eslint . --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GitPaulo/Vulcan.git"
    },
    "keywords": [
        "vulcan",
        "discord",
        "bot",
        "discord-js",
        "discordjs",
        "heroku"
    ],
    "author": "Paulo & Carlos",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GitPaulo/Vulcan/issues"
    },
    "homepage": "https://github.com/GitPaulo/Vulcan#readme",
    "dependencies": {
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "discord.js": "github:discordjs/discord.js",
        "ffmpeg-static": "^2.4.0",
        "github-api": "^3.2.2",
        "got": "^9.6.0",
        "js-yaml": "^3.13.1",
        "mongoose": "^5.6.3",
        "node-opus": "^0.3.2",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "ytdl-core": "^0.29.2",
        "ytdl-core-discord": "^1.0.3"
    },
    "devDependencies": {
        "eslint": "^6.0.1",
        "eslint-config-mudamuda": "^1.0.6",
        "eslint-plugin-security": "^1.4.0",
        "nodemon": "^1.19.1"
    }
}
