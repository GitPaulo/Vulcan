{
    "name": "Vulcan",
    "version": "1.0.0",
    "description": "Collaboration discord bot by Paulo & Carlos, using discord.js.",
    "main": "vulcan.js",
    "scripts": {
        "start": "node --require ./plugins/scripts/globals.js bot.js",
        "dev": "nodemon --require ./plugins/scripts/globals.js bot.js",
        "test": "node --require ./plugins/scripts/globals.js tests/loading.js",
        "linter": "./node_modules/.bin/eslint .",
        "linter:fix": "./node_modules/.bin/eslint . --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GitPaulo/Vulcan.git"
    },
    "keywords": [
        "vulcan",
        "discord",
        "bot",
        "discord-js",
        "discordjs",
        "heroku"
    ],
    "author": "Paulo & Carlos",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GitPaulo/Vulcan/issues"
    },
    "homepage": "https://github.com/GitPaulo/Vulcan#readme",
    "dependencies": {
        "chalk": "^2.4.2",
        "discord.js": "discordjs/discord.js",
        "github-api": "^3.2.2",
        "js-yaml": "^3.13.1",
        "mongoose": "^5.6.0"
    },
    "devDependencies": {
        "eslint": "^5.16.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-standard": "^4.0.0",
        "nodemon": "^1.19.1"
    }
