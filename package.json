{
  "name": "Vulcan",
  "version": "1.0.0",
  "description": "Collaboration discord bot by Paulo & Carlos, using discord.js.",
  "main": "bot.js",
  "scripts": {
    "_COMMENT0": " ============================ BUILD SCRIPTS  ============================",
    "wintools": "npm install -g windows-build-tools",
    "fixhttps": "npm config set registry http://registry.npmjs.org/",
    "_COMMENT1": " ============================ RUN SCRIPTS  ============================",
    "start": "node index.js ./bot.js",
    "dev": "nodemon index.js ./bot.js",
    "production": "pm2 start index.js --name=vulcan -- ./bot.js",
    "_COMMENT2": " ============================ PRODUCTION SCRIPTS  ============================",
    "production:list": "pm2 ls",
    "production:restart": "pm2 restart vulcan",
    "production:stop": "pm2 stop vulcan",
    "production:delete": "pm2 delete vulcan",
    "production:logs": "pm2 logs vulcan --lines 30",
    "production:show": "pm2 show vulcan",
    "production:monitor": "pm2 monitor vulcan",
    "_COMMENT3": " ============================ TEST SCRIPTS  ============================",
    "test": "node index.js ./tests/loading.js",
    "test:music": "node index.js ./tests/musicDependencies.js",
    "test:discord": "node index.js ./tests/discordConnection.js",
    "_COMMENT4": " ============================ CODE ENV SCRIPTS  ============================",
    "linter": "eslint .",
    "linter:errors-only": "eslint . --quiet",
    "linter:setup": "npm install eslint --save-dev",
    "linter:init": "eslint --init",
    "linter:fix": "eslint . --fix",
    "snyk:protect": "snyk protect",
    "snyk:auth": "snyk auth",
    "snyk:snapshot": "snyk monitor",
    "snyk:wizard": "snyk wizard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitPaulo/Vulcan.git"
  },
  "keywords": [
    "vulcan",
    "discord",
    "bot",
    "discord-js",
    "discordjs"
  ],
  "author": "Paulo & Carlos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GitPaulo/Vulcan/issues"
  },
  "homepage": "https://github.com/GitPaulo/Vulcan#readme",
  "dependencies": {
    "archiver": "^3.1.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "github:discordjs/discord.js",
    "github-api": "^3.2.2",
    "got": "^9.6.0",
    "js-yaml": "^3.13.1",
    "mongoose": "^5.6.10",
    "opusscript": "0.0.7",
    "oauth": "^0.9.15",
    "pem": "^1.14.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "ytdl-core": "^0.29.5",
    "ytdl-core-discord": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-mudamuda": "^1.0.13",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^1.19.1",
    "pm2": "^3.5.1",
    "snyk": "^1.217.3"
  },
  "snyk": true
}
